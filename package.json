{
  "name": "@xbzoom/xbzui",
  "version": "0.1.0",
  "description": "react component",
  "private": false,
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "eslint": "eslint --ext .tsx,.ts,.jsx,.js --fix ./src",
    "start": "webpack-dev-server --config config/webpack.config.dev.js --color",
    "lib": "tsc --outDir lib/ --module commonjs",
    "es": "tsc",
    "umd": "webpack --config config/webpack.config.prod.js --color",
    "gulp": "cd config && gulp",
    "build": "npm run umd && npm run lib && npm run es && npm run gulp",
    "pub": "npm publish --access=public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,jsx,js}" : [
      "npm run eslint",
      "prettier .prettierrc.js --write",
      "git add"
    ]
  },
  "keywords": [
    "react",
    "component",
    "webpack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shenck215/react-component.git"
  },
  "author": "shenck",
  "license": "ISC",
